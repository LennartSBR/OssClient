{
  "name": "ossclient",
  "productName": "OSS-Client",
  "version": "1.1.0",
  "description": "Digital in der Schule",
  "main": "src/main.js",
  "scripts": {
    "start": "electron ."
  },
  "build": {
    "appId": "com.lslabs.ossclient",
    "win": {
      "target": [
        "nsis"
      ],
