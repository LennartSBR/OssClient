    "name": "LsLabs",
    "email": "lennart.schmehr@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^27.1.2",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2"
  }
}
